@model MentorMe.Models.ModelAreaOfInterest

@{
    ViewBag.Title = "Interest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Interest</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Area Of Interest</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.KeySkill)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.KeySkill)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Experiance)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Experiance)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Skip", "Dashboard")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
